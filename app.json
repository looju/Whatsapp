{
  "expo": {
    "name": "Whatsapp",
    "slug": "Whatsapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Whatsapp requires permission to access your photos."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Whatsapp requires permission to access your contacts."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "Whatsapp requires permissionto access your contacts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.loju.Whatsapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9fcbc7f8-e36b-41fe-965d-75092da68536"
      }
    }
  }
}
